{
  "expo": {
    "name": "Safe Cert",
    "slug": "Safecert",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/safecertLogo.png",
    "scheme": "safecertapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.safecert",
      "splash": {
        "image": "./assets/images/splash-screen.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "backgroundColor": "#ffffff"
        }
      }
    },
    "android": {
      "package": "com.yourcompany.safecert",
      "versionCode": 3,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/safecertLogo.png"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECORD_AUDIO"
      ],
      "splash": {
        "image": "./assets/images/splash-screen.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "backgroundColor": "#ffffff"
        }
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/safecertLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-screen.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#ffffff"
          }
        }
      ],
      "expo-web-browser",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a84456e8-91af-4027-a224-125cc5a5de47"
      }
    }
  }
}
